

create value 'coordinate';

create validator 'between' (min, max) as ({
	(value >= min) and (value <= max);
});

create validator 'float' as ({
	regex ('/[0-9]+\.[0-9]+/');
});

create validator 'coordinate' as ({
	(validator\between (-90, 90)) and (validator\float);
});

update value 'coordinate' set validator to (validator\coordinate);

replace validator 'float' as ({
	regex ('/[0-9]+[\.[0-9]+]?/');
});



